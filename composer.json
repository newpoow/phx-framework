{
  "name": "newpoow/phx-framework",
  "description": "A simple and efficient web framework",
  "license": "MIT",
  "homepage": "https://github.com/newpoow/phx-framework",
  "type": "library",
  "authors": [{
    "name": "Ney Pinheiro",
    "homepage": "https://neypinheiro.dev"
  }],
  "keywords": [
    "micro", "framework", "phx", "simple", "api", "rest", "mvc"
  ],
  "autoload": {
    "psr-4": {
      "Phx\\": "src"
    }
  },
  "require": {
    "php": ">=8.1",
    "psr/container": "^2.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^2.0",
    "psr/http-server-middleware": "^1.0"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
